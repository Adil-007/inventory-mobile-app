{
  "expo": {
    "name": "Ease Stock",
    "slug": "ease-stock",
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "easestock",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.easestock.app",
      "buildNumber": "2"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.easestock.app",
      "versionCode": 1
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
